@{
}
@model BooksViewModel

<h1>Bookstore</h1>

@foreach (Book x in Model.Books)
{
    <h3>@x.Title</h3>

    <ul>
        <li>Author: @x.Author</li>
        <li>Publisher: @x.Publisher</li>
        <li>Isbn: @x.Isbn</li>
        <li>Category: @x.Category</li>
        <li>Pages: @x.PageCount</li>
        <li>Price: @x.Price</li>
    </ul>
    <br />
}

<a asp-action="Index" asp-route-pageNum="1">1</a>

<div page-blah="@Model.PageInfo"></div>